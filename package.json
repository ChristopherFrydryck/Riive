{
  "name": "riive",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "devices": "adb devices && xcrun instruments -s devices",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "clean-ios": "cd ios && xcodebuild clean && cd .. && rm -rf /Users/christopherfrydryck/Library/Developer/Xcode/DerivedData/* && watchman watch-del-all && cd ios && pod install && cd ..",
    "firebase-projects": "firebase projects:list",
    "firebase-function-env-vars": "firebase functions:config:get",
    "firebase-dev": "firebase use dev",
    "firebase-prod": "firebase use prod",
    "android": "ENVFILE=.env.development react-native run-android --variant=devDebug --appIdSuffix development",
    "android-device": "ENVFILE=.env.development react-native run-android --deviceId=$DEVICEID --appIdSuffix development --variant=devDebug",
    "android-device-dev": "ENVFILE=.env.development react-native run-android --deviceId=$DEVICEID --appIdSuffix development --variant=devDebug",
    "android-device-prod": "ENVFILE=.env.production react-native run-android --deviceId=$DEVICEID --variant=prodDebug",
    "android-device-staging": "ENVFILE=.env.staging react-native run-android --appIdSuffix staging --deviceId=$DEVICEID --variant=stageDebug",
    "android-prod": "ENVFILE=.env.production react-native run-android --variant=prodDebug",
    "android-staging": "ENVFILE=.env.staging react-native run-android --appIdSuffix staging  --variant=stageDebug",
    "android-dev": "ENVFILE=.env.development react-native run-android --appIdSuffix development --variant=devDebug",
    "ios": "ENVFILE=.env.development react-native run-ios --scheme 'riive-development'",
    "ios-device": "ENVFILE=.env.development react-native run-ios --device --scheme 'riive-development'",
    "ios-staging": "ENVFILE=.env.staging react-native run-ios --scheme 'riive-staging'",
    "ios-device-staging": "ENVFILE=.env.staging react-native run-ios --device --scheme 'riive-staging'",
    "ios-dev": "ENVFILE=.env.development react-native run-ios --scheme 'riive-development'",
    "ios-device-dev": "ENVFILE=.env.development react-native run-ios --device --scheme 'riive-development'",
    "ios-prod": "ENVFILE=.env.production react-native run-ios",
    "ios-device-prod": "ENVFILE=.env.production react-native run-ios --device",
    "start": "react-native start",
    "clean-start": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* &&  rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && npm cache clean --force &&  npm install --legacy-peer-deps && npm start -- --reset-cache",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-clipboard/clipboard": "^1.9.0",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-firebase/app": "^13.0.1",
    "@react-native-firebase/auth": "^13.0.1",
    "@react-native-firebase/firestore": "^13.0.1",
    "@react-native-firebase/messaging": "^13.0.1",
    "@react-native-firebase/storage": "^13.0.1",
    "axios": "^0.24.0",
    "fbjs": "^3.0.0",
    "geofirestore": "^4.4.2",
    "intl": "^1.2.5",
    "mobx": "^6.3.7",
    "mobx-react": "^5.4.4",
    "patch-package": "^6.4.7",
    "react": "17.0.2",
    "react-native": "0.66.0",
    "react-native-actions-sheet": "^0.5.7",
    "react-native-config": "^1.4.5",
    "react-native-device-info": "^8.4.7",
    "react-native-dialog-input": "^1.0.8",
    "react-native-flash-message": "^0.2.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-google-places-autocomplete": "^2.4.1",
    "react-native-image-crop-picker": "^0.37.2",
    "react-native-keyboard-aware-scroll-view": "github:APSL/react-native-keyboard-aware-scroll-view#pull/501/head",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-maps": "0.29.4",
    "react-native-masked-text": "^1.13.0",
    "react-native-modal-selector": "^2.1.0",
    "react-native-paper": "^4.10.0",
    "react-native-permissions": "^3.1.0",
    "react-native-reanimated": "~2.2.4",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.9.0",
    "react-native-svg": "12.1.1",
    "react-native-svg-animated-linear-gradient": "^0.4.0",
    "react-native-swipeable-row": "^0.8.1",
    "react-native-unimodules": "^0.14.10",
    "react-native-vector-icons": "^9.0.0",
    "react-navigation": "^4.4.4",
    "react-navigation-stack": "^2.10.4",
    "react-navigation-tabs": "^2.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2"
  },
  "resolutions": {
    "react-native-svg": "9.9.5",
    "react-native-safe-area-view": "^2.0.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
