{
  "name": "riive",
  "version": "1.0.2",
  "private": true,
  "beta": true,
  "scripts": {
    "devices": "adb devices && xcrun xctrace list devices",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "clean-ios": "cd ios && xcodebuild clean && cd .. && watchman watch-del-all && cd ios && pod install && cd ..",
    "check-packages": "ncu",
    "firebase-projects": "firebase projects:list",
    "firebase-function-env-vars": "firebase functions:config:get",
    "firebase-dev": "firebase use dev",
    "firebase-prod": "firebase use prod",
    "android": "ENVFILE=.env.development react-native run-android --variant=devDebug --appIdSuffix development",
    "android-build": "cd android && ENVFILE=.env.production ./gradlew bundleRelease && cd ..",
    "android-release": "ENVFILE=.env.production react-native run-android --variant=prodRelease",
    "android-device-release": "ENVFILE=.env.production react-native run-android --deviceId=$DEVICEID --variant=prodRelease",
    "android-device": "ENVFILE=.env.development react-native run-android --deviceId=$DEVICEID --appIdSuffix development --variant=devDebug",
    "android-device-dev": "ENVFILE=.env.development react-native run-android --deviceId=$DEVICEID --appIdSuffix development --variant=devDebug",
    "android-device-prod": "ENVFILE=.env.production react-native run-android --deviceId=$DEVICEID --variant=prodDebug",
    "android-device-staging": "ENVFILE=.env.staging react-native run-android --appIdSuffix staging --deviceId=$DEVICEID --variant=stageDebug",
    "android-prod": "ENVFILE=.env.production react-native run-android --variant=prodDebug",
    "android-staging": "ENVFILE=.env.staging react-native run-android --appIdSuffix staging  --variant=stageDebug",
    "android-dev": "ENVFILE=.env.development react-native run-android --appIdSuffix development --variant=devDebug",
    "ios": "ENVFILE=.env.development react-native run-ios --scheme 'riive-development' --simulator 'iPhone 14 Pro Max'",
    "ios-device": "ENVFILE=.env.development react-native run-ios --udid 00008101-000D25693E80001E --scheme 'riive-development'",
    "ios-staging": "ENVFILE=.env.staging react-native run-ios --scheme 'riive-staging' --simulator 'iPhone 14 Pro Max'",
    "ios-device-staging": "ENVFILE=.env.staging react-native run-ios --udid 00008101-000D25693E80001E --scheme 'riive-staging'",
    "ios-dev": "ENVFILE=.env.development react-native run-ios --scheme 'riive-development' --simulator 'iPhone 14 Pro Max'",
    "ios-device-dev": "ENVFILE=.env.development react-native run-ios --udid 00008101-000D25693E80001E --scheme 'riive-development'",
    "ios-prod": "ENVFILE=.env.production react-native run-ios --simulator 'iPhone 14 Pro Max'",
    "ios-device-prod": "ENVFILE=.env.production react-native run-ios --udid 00008101-000D25693E80001E",
    "start": "react-native start",
    "clean-start": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* &&  rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && npm cache clean --force &&  npm install --legacy-peer-deps && npm start -- --reset-cache && npx patch-package",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-clipboard/clipboard": "^1.11.1",
    "@react-native-community/geolocation": "^3.0.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-firebase/app": "14.11.1",
    "@react-native-firebase/auth": "14.11.1",
    "@react-native-firebase/firestore": "14.11.1",
    "@react-native-firebase/messaging": "14.11.1",
    "@react-native-firebase/storage": "14.11.1",
    "axios": "^0.27.2",
    "deprecated-react-native-prop-types": "^2.3.0",
    "fbjs": "^3.0.4",
    "geofirestore": "^5.2.0",
    "intl": "^1.2.5",
    "mobx": "^6.6.2",
    "mobx-react": "^7.5.3",
    "patch-package": "^6.4.7",
    "react": "18.2.0",
    "react-native": "0.70.1",
    "react-native-actions-sheet": "^0.8.7",
    "react-native-config": "^1.4.6",
    "react-native-device-info": "^10.2.0",
    "react-native-dialog-input": "^1.0.8",
    "react-native-flash-message": "^0.3.1",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.6.2",
    "react-native-google-places-autocomplete": "^2.4.1",
    "react-native-haptic-feedback": "^1.14.0",
    "react-native-image-crop-picker": "^0.38.0",
    "react-native-keyboard-aware-scroll-view": "github:APSL/react-native-keyboard-aware-scroll-view#pull/501/head",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-maps": "0.29.4",
    "react-native-masked-text": "^1.13.0",
    "react-native-modal-selector": "^2.1.1",
    "react-native-paper": "^4.12.4",
    "react-native-permissions": "^3.6.1",
    "react-native-reanimated": "~2.10.0",
    "react-native-safe-area-context": "^4.3.4",
    "react-native-screens": "^3.17.0",
    "react-native-svg": "13.2.0",
    "react-native-svg-animated-linear-gradient": "^0.4.0",
    "react-native-swipeable-row": "^0.8.1",
    "react-native-unimodules": "^0.14.10",
    "react-native-vector-icons": "^9.2.0",
    "react-navigation": "^4.4.4",
    "react-navigation-stack": "^2.10.4",
    "react-navigation-tabs": "^2.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-proposal-decorators": "^7.19.1",
    "@babel/runtime": "^7.19.0",
    "@react-native-community/eslint-config": "^3.1.0",
    "babel-jest": "^29.0.3",
    "eslint": "8.24.0",
    "jest": "^29.0.3",
    "metro-react-native-babel-preset": "^0.72.3",
    "react-test-renderer": "18.2.0"
  },
  "resolutions": {
    "react-native-svg": "9.9.5",
    "react-native-safe-area-view": "^2.0.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
